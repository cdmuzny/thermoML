            <xsd:element  name="IUPACSymbol"  minOccurs="0">  
       <xsd:complexType> 
        <xsd:sequence> 
         <xsd:element ref="mml:math"/> 
        </xsd:sequence> 
       </xsd:complexType> 
      </xsd:element> 
     </xsd:sequence> 
    </xsd:complexType> 
      </xsd:element>  
      <xsd:element  name="EqConstant"  minOccurs="0"  maxOccurs="unbounded">  
