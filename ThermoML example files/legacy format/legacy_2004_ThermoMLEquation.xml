    </xsd:complexType> 
      </xsd:element>  
      <xsd:element  name="EqMathContent"  maxOccurs="unbounded">  
    <xsd:complexType> 
     <xsd:sequence> 
            <xsd:element  ref="mml:math"/>  
     </xsd:sequence> 
65
    </xsd:complexType> 
      </xsd:element>  
