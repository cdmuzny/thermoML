                    <xsd:annotation>
                        <xsd:documentation>EAP: Standard method(s) of measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="Calorimetry measurements"/>
                            <xsd:enumeration
                                value="Characterized using X-ray absorption spectroscopy"/>
                            <xsd:enumeration value="Characterized using Neutron diffraction"/>
                            <xsd:enumeration value="Characterized using X-ray diffraction"/>
                            <xsd:enumeration value="Characterized using X-ray tomography"/>
                            <xsd:enumeration value="Determined from cooling curves"/>
                            <xsd:enumeration value="Determined from DSC"/>
                            <xsd:enumeration value="Determined from DTA"/>
                            <xsd:enumeration value="Determined from Dilatometry"/>
                            <xsd:enumeration value="Determined from Interferometry"/>
