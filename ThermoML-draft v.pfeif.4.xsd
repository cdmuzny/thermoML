    <xsd:complexType name="dataReportType">
        <xsd:annotation>
            <xsd:documentation>Type information for root container</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="citation" type="tml:citationType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Elements to convey provenance about communicated data</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="chemical" type="tml:chemicalType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Elements of chemicals referenced in the data</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="system" type="tml:systemType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Elements relating the chemical make-up to the final system</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="phase" type="tml:phaseType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Elements of Phase or phases observed</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dataset" type="tml:datasetType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>The measurements of a system reported in a citation</xsd:documentation>
                </xsd:annotation>
                <!--                <xsd:key name="variableIDKey">
                    <xsd:selector xpath="variable"/>
                    <xsd:field xpath="@id"/>
                </xsd:key>
                <xsd:keyref name="valueVariableIDKeyref" refer="tml:variableIDKey">
                    <xsd:selector xpath="point/var/value"/>
                    <xsd:field xpath="@ref"/>
                </xsd:keyref>
-->
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <!-- Citation definition -->
    <xsd:complexType name="citationType">
        <xsd:sequence>
            <xsd:element name="TRCRefID" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>TRC Reference Identifier</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="yearPub" type="xsd:integer">
                            <xsd:annotation>
                                <xsd:documentation>Integer year of publication</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="author1" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation>First 3 characters of Author 1 last name</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="author2" type="xsd:string">
                            <xsd:annotation>
                                <xsd:documentation>First 3 characters of Author 2 last name</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="authorN" type="xsd:integer">
                            <xsd:annotation>
                                <xsd:documentation>Integer identifier to distinguish conflicts</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="documentType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The type of publication</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="book"/>
                        <xsd:enumeration value="journal"/>
                        <xsd:enumeration value="report"/>
                        <xsd:enumeration value="patent"/>
                        <xsd:enumeration value="thesis"/>
                        <xsd:enumeration value="conferenceProceedings"/>
                        <xsd:enumeration value="archivedDocument"/>
                        <xsd:enumeration value="personalCorrespondence"/>
                        <xsd:enumeration value="publishedTranslation"/>
                        <xsd:enumeration value="unspecified"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="sourceType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The source status type for a citation</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Original"/>
                        <xsd:enumeration value="ChemicalAbstracts"/>
                        <xsd:enumeration value="Other"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="documentOrigin" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Company, institution, or conference name</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="author" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Author of publication</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="namePublication" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Name of the publication.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="yearPublished" type="xsd:gYear" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Publication year</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="date" type="xsd:date" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Date of the citation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="title" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Title of the publication</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="abstract" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>An abstract of the publication</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="keyword" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>EAP: list of keywords listed with publication</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="language" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Language of publication</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Chinese"/>
                        <xsd:enumeration value="English"/>
                        <xsd:enumeration value="French"/>
                        <xsd:enumeration value="German"/>
                        <xsd:enumeration value="Japanese"/>
                        <xsd:enumeration value="Polish"/>
                        <xsd:enumeration value="Russian"/>
                        <xsd:enumeration value="Other language"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="DOI" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>DOI</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="URL" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>URL to the publication</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CASCitation" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>The Chemical Abstracts Service citation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="IDNum" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Identification number, e.g., a patent number or a document number</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="location" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Reference to a location</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="volume" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Volume number</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="pages" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Page range where the publication can be found</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0">
                <xsd:element name="book">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="chapter" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Chapter number</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="edition" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Edition number of the book</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="editor" type="xsd:string" minOccurs="0"
                                maxOccurs="unbounded">
                                <xsd:annotation>
                                    <xsd:documentation>Editor of the book</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="ISBN" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The International Standard Book Number</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="publisher" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Publisher name and city</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="journal">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="ISSN" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The International Standard Subscription Number</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="issue" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Issue number, usually indicates month</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="CODEN" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>The CODEN identification of the journal</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="thesis">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="degree" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Academic degree designation, e.g., MS or PhD</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="institution" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Academic degree granting institution</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="UMIPublicationNumber" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>University Microfilms International Publication Number</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:positiveInteger" use="required">
            <xsd:annotation>
                <xsd:documentation>Index to link citations to data</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <!-- Chemical definition -->
    <xsd:complexType name="chemicalType">
        <xsd:annotation>
            <xsd:documentation>Material component characteristics that serves to identify the component</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="RegNum" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>EAP: Registery chemical identifier (Organizational or CASRN)</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="CASRNum" type="xsd:integer" minOccurs="0">
                            <xsd:annotation>
                                <xsd:documentation>EAP: Chemical Abstract Services Registry Number (CASRN)</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:sequence minOccurs="0">
                            <xsd:element name="organizaitonNumber" type="xsd:integer">
                                <xsd:annotation>
                                    <xsd:documentation>Registry number as given by organization</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="organizationName" type="xsd:string" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Organization name that designated organization number</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="PubChemID" type="xsd:integer" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>EAP: Chemical identificaiton number assigned via PubChem database</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="standardInChI" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:appinfo>Standard InChI string</xsd:appinfo>
                    <xsd:documentation>IUPAC International Chemical Identifier</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="standardInChIKey" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:appinfo>Standard InChI key</xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="organizationID" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="organizationIdentifier" type="xsd:string"/>
                        <xsd:element name="organization" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="CASName" type="xsd:string" minOccurs="0"/>
            <xsd:element name="IUPACName" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:appinfo>IUPAC name, string</xsd:appinfo>
                    <xsd:documentation>International Union of Physics and Applied Chemistry name</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="commonName" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:appinfo>Common name, string</xsd:appinfo>
                    <xsd:documentation>Common name</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="formulaMolecular" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:appinfo>Molecular formula, string</xsd:appinfo>
                    <xsd:documentation>Chemical molecular formula</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SMILES" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:appinfo>SMILES notation, string</xsd:appinfo>
                    <xsd:documentation>SMILES notation</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0">
                <xsd:element name="polymer">
                    <xsd:complexType>
                        <xsd:annotation>
                            <xsd:documentation>Polymer-specific information</xsd:documentation>
                        </xsd:annotation>
                        <xsd:sequence>
                            <xsd:element name="numberAvgMolMass" type="xsd:float" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Number average molecular mass, kg/kmol</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="peakAvgMolMass" type="xsd:float" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Peak average molecular mass, kg/kmol</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="viscosityAvgMolMass" type="xsd:float" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Viscosity average molecular mass, kg/kmol</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="massAvgMolMass" type="xsd:float" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Weight average molecular mass, kg/kmol</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="zAvgMolMass" type="xsd:float" minOccurs="0">
                                <xsd:annotation>
                                    <xsd:documentation>Z average molecular mass, kg/kmol</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="molarMassDispersity" type="xsd:float" minOccurs="0"/>
                            <xsd:element name="degOfPolymerizationDispersity" type="xsd:float"
                                minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="ion">
                    <xsd:complexType>
                        <xsd:annotation>
                            <xsd:documentation>Ion-specific information</xsd:documentation>
                        </xsd:annotation>
                        <xsd:sequence>
                            <xsd:element name="charge" type="xsd:integer"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="biomaterial">
                    <xsd:complexType>
                        <xsd:annotation>
                            <xsd:documentation>Bio species information</xsd:documentation>
                        </xsd:annotation>
                        <xsd:sequence>
                            <xsd:element name="ECNumber" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="PDBIdentifier" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:choice>
            <xsd:element name="speciationState" minOccurs="0">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="equilibrium"/>
                        <xsd:enumeration value="single species"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:positiveInteger" use="required">
            <xsd:annotation>
                <xsd:documentation>Index to link compounds to data</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <!-- System definition -->
    <xsd:complexType name="systemType">
        <xsd:choice>
            <xsd:element name="multicomponentSubstance" type="tml:multicomponentSubstanceType"/>
            <xsd:element name="process" type="tml:processType"/>
            <xsd:element name="reaction" type="tml:reactionType"/>
            <xsd:element name="sample" type="tml:sampleType"/>
        </xsd:choice>
        <xsd:attribute name="id"/>
    </xsd:complexType>
    <xsd:complexType name="multicomponentSubstanceType">
        <xsd:sequence>
            <xsd:element name="component" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:choice>
                            <xsd:element name="chemical">
                                <xsd:complexType>
                                    <xsd:attribute name="ref" type="xsd:positiveInteger"
                                        use="required"/>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="system">
                                <xsd:complexType>
                                    <xsd:attribute name="ref" type="xsd:positiveInteger"
                                        use="required"/>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:choice>
                        <xsd:element name="amount" type="xsd:float"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="type">
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="alloy"/>
                    <xsd:enumeration value="clathrate"/>
                    <xsd:enumeration value="complex"/>
                    <xsd:enumeration value="crystal"/>
                    <xsd:enumeration value="solution"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="compositionBasis">
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="number of molecules"/>
                    <xsd:enumeration value="mole fraction"/>
                    <xsd:enumeration value="mass fraction"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="processType">
        <!-- This needs to be populated -->
        <xsd:annotation>
            <xsd:documentation>Material specimen processing prior to measurement: includes any special preparation processes, thermal processing, mechanical processing, and purification methods.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:choice>
                <xsd:element name="chemical">
                    <xsd:complexType>
                        <xsd:attribute name="ref" type="xsd:positiveInteger" use="required"/>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="system">
                    <xsd:complexType>
                        <xsd:attribute name="ref" type="xsd:positiveInteger" use="required"/>
                    </xsd:complexType>
                </xsd:element>
            </xsd:choice>
            <xsd:choice>
                <xsd:element name="heatingCooling">
                    <xsd:annotation>
                        <xsd:documentation>Heating/Cooling</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:choice minOccurs="0">
                            <xsd:element name="cooling">
                                <xsd:annotation>
                                    <xsd:documentation>Cooling</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Air"/>
                                        <xsd:enumeration value="Casting"/>
                                        <xsd:enumeration value="Furnace"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="modifiedSolidification">
                                <xsd:annotation>
                                    <xsd:documentation>Modified Solidification</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Seeded Solidification"/>
                                        <xsd:enumeration value="Directional Solidification"/>
                                        <xsd:enumeration value="Single Crystal Solidification"/>
                                        <xsd:enumeration value="Zone Refining"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="quenching">
                                <xsd:annotation>
                                    <xsd:documentation>Quenching</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Ice Quenching"/>
                                        <xsd:enumeration value="Liquid Nitrogen Quenching"/>
                                        <xsd:enumeration value="Oil Quenching"/>
                                        <xsd:enumeration value="Water Quenching"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="rapidCooling">
                                <xsd:annotation>
                                    <xsd:documentation>Rapid Cooling</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Atomization Processing"/>
                                        <xsd:enumeration value="Melt Spinning"/>
                                        <xsd:enumeration value="Splat Cooling"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="rapidHeating">
                                <xsd:annotation>
                                    <xsd:documentation>Rapid Heating</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Electron Beam Processing"/>
                                        <xsd:enumeration value="Laser Processing"/>
                                        <xsd:enumeration value="Microwave Processing"/>
                                        <xsd:enumeration value="Resistive Heating"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="heating">
                                <xsd:annotation>
                                    <xsd:documentation>Heating</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                        </xsd:choice>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="mechanicalProcessing">
                    <xsd:annotation>
                        <xsd:documentation>Mechanical Processing</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:choice minOccurs="0">
                            <xsd:element name="acousticWave">
                                <xsd:annotation>
                                    <xsd:documentation>Acoustic Wave</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Shock Wave"/>
                                        <xsd:enumeration value="Ultrasonic"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="deforming">
                                <xsd:annotation>
                                    <xsd:documentation>Deforming</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Extruding"/>
                                        <xsd:enumeration value="Forming"/>
                                        <xsd:enumeration value="Rolling"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="machining">
                                <xsd:annotation>
                                    <xsd:documentation>Machining</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Machining Surface"/>
                                        <xsd:enumeration value="Machining to Shape"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="millingGrinding">
                                <xsd:annotation>
                                    <xsd:documentation>Milling/Grinding</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Ball milling"/>
                                        <xsd:enumeration value="Filing"/>
                                        <xsd:enumeration value="Mortar and pestle"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="pressing">
                                <xsd:annotation>
                                    <xsd:documentation>Pressing</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Bonding"/>
                                        <xsd:enumeration value="Die Pressing"/>
                                        <xsd:enumeration value="Isostatic Pressing"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                        </xsd:choice>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="preparation">
                    <xsd:annotation>
                        <xsd:documentation>Preparation</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:choice minOccurs="0">
                            <xsd:element name="chemical">
                                <xsd:annotation>
                                    <xsd:documentation>Chemical</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Cleaning"/>
                                        <xsd:enumeration value="Etching"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="depositionProcessing">
                                <xsd:annotation>
                                    <xsd:documentation>Deposition Processing</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Sputter Depositing"/>
                                        <xsd:enumeration value="Chemical Vapor Depositing"/>
                                        <xsd:enumeration value="Electrodeposition"/>
                                        <xsd:enumeration value="Ion Beam Depositing"/>
                                        <xsd:enumeration value="Physical Vapor Depositing"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="meltingCasting">
                                <xsd:annotation>
                                    <xsd:documentation>Melting &amp; Casting</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Arc Melting"/>
                                        <xsd:enumeration value="Casting"/>
                                        <xsd:enumeration value="Induction Melting"/>
                                        <xsd:enumeration value="Radiation/Furnace Melting"/>
                                        <xsd:enumeration value="Vacuum Degassing"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="modifiedSolidification">
                                <xsd:annotation>
                                    <xsd:documentation>Modified Solidification</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Seeded Solidification"/>
                                        <xsd:enumeration value="Directional Solidification"/>
                                        <xsd:enumeration value="Single Crystal Solidification"/>
                                        <xsd:enumeration value="Zone Refining"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="powder">
                                <xsd:annotation>
                                    <xsd:documentation>Powder</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Atomization Processing"/>
                                        <xsd:enumeration value="Chemical Reducing"/>
                                        <xsd:enumeration value="Electrolysis  Processing"/>
                                        <xsd:enumeration value="Mechanical Crushing"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="rapidHeating">
                                <xsd:annotation>
                                    <xsd:documentation>Rapid Heating</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Electron Beam Processing"/>
                                        <xsd:enumeration value="Laser Processing"/>
                                        <xsd:enumeration value="Microwave Processing"/>
                                        <xsd:enumeration value="Resistive Heating"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                        </xsd:choice>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="thermalProcessing">
                    <xsd:annotation>
                        <xsd:documentation>Thermal Processing</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:choice minOccurs="0">
                            <xsd:element name="annealing">
                                <xsd:annotation>
                                    <xsd:documentation>Annealing</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Precipitation Annealing"/>
                                        <xsd:enumeration value="Relaxation Annealing"/>
                                        <xsd:enumeration value="Solution Annealing"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="melting">
                                <xsd:annotation>
                                    <xsd:documentation>Melting</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Shock Wave"/>
                                        <xsd:enumeration value="Ultrasonic"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="sintering">
                                <xsd:annotation>
                                    <xsd:documentation>Sintering</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value=""/>
                                        <xsd:enumeration value="Laser Sintering"/>
                                        <xsd:enumeration value="Radiation Sintering (Furnace)"/>
                                        <xsd:enumeration value="Resistance Sintering"/>
                                        <xsd:enumeration value="Spark Plasma Sintering"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                        </xsd:choice>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="purification">
                    <xsd:complexType>
                        <xsd:choice>
                            <xsd:element name="method">
                                <xsd:simpleType>
                                    <xsd:annotation>
                                        <xsd:documentation>Purification method(s) used to purify sample</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value="Impurity adsorption"/>
                                        <xsd:enumeration value="Vacuum degasification"/>
                                        <xsd:enumeration value="Chemical reagent treatment"/>
                                        <xsd:enumeration value="Crystallization from melt"/>
                                        <xsd:enumeration value="Crystallization from solution"/>
                                        <xsd:enumeration value="Liquid chromatography"/>
                                        <xsd:enumeration value="Dried with chemical reagent"/>
                                        <xsd:enumeration value="Dried in a desiccator"/>
                                        <xsd:enumeration value="Dried by oven heating"/>
                                        <xsd:enumeration value="Dried by vacuum heating"/>
                                        <xsd:enumeration
                                            value="De-gassed by boiling or ultrasonically"/>
                                        <xsd:enumeration value="De-gassed by evacuation"/>
                                        <xsd:enumeration
                                            value="De-gassed by freezing and melting in vacuum"/>
                                        <xsd:enumeration value="Fractional crystallization"/>
                                        <xsd:enumeration value="Fractional distillation"/>
                                        <xsd:enumeration value="Molecular sieve treatment"/>
                                        <xsd:enumeration value="Unspecified"/>
                                        <xsd:enumeration value="Preparative gas chromatography"/>
                                        <xsd:enumeration value="Sublimation"/>
                                        <xsd:enumeration value="Steam distillation"/>
                                        <xsd:enumeration value="Solvent extraction"/>
                                        <xsd:enumeration value="Salting out of solution"/>
                                        <xsd:enumeration value="Zone refining"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="otherMethod" type="xsd:string"/>
                        </xsd:choice>
                    </xsd:complexType>
                </xsd:element>
            </xsd:choice>
            <xsd:element name="temperature" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="info" type="tml:temperatureType"/>
                        <xsd:element name="val" type="tml:valueType"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="pressure" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="info" type="tml:pressureType"/>
                        <xsd:element name="val" type="tml:valueType"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="time" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="unit">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value="minutes"/>
                                    <xsd:enumeration value="hours"/>
                                    <xsd:enumeration value="days"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="val" type="tml:valueType"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="rate" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="unit">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value="K/s"/>
                                    <xsd:enumeration value="K/min"/>
                                    <xsd:enumeration value="K/hr"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="val" type="tml:valueType"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="crucibleMaterial" minOccurs="0">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Al2O3"/>
                        <xsd:enumeration value="aluminum"/>
                        <xsd:enumeration value="BeO"/>
                        <xsd:enumeration value="BN"/>
                        <xsd:enumeration value="brass"/>
                        <xsd:enumeration value="copper"/>
                        <xsd:enumeration value="graphite"/>
