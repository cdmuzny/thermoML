            <xsd:documentation>EAP: used for molar entropy of transition, molar entropy of vaporization/transition, molar entropy, apparent molar entropy, excess molar entropy, partial molar entropy, relative partial molar entropy, standard state molar entropy, </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="J/K/mol">
                <xsd:annotation>
                    <xsd:documentation>EAP: measured unit of property</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="kJ/K/mol"/>
                        <xsd:enumeration value="J/K/mol"/>
                        <xsd:enumeration value="kJ/K/g"/>
                        <xsd:enumeration value="J/K/g"/>
