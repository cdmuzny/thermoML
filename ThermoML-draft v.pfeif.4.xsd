                                    <xsd:enumeration value="TiO2 (rutile)"/>
                                    <xsd:enumeration value="TiSi2"/>
                                    <xsd:enumeration value="TlSe"/>
                                    <xsd:enumeration value="UB12"/>
                                    <xsd:enumeration value="W"/>
                                    <xsd:enumeration value="W2B5"/>
                                    <xsd:enumeration value="W3Si3"/>
                                    <xsd:enumeration value="WC"/>
                                    <xsd:enumeration value="Zn3P2"/>
                                    <xsd:enumeration value="ZnS (sphalerite)"/>
                                    <xsd:enumeration value="ZnS (wurtzite)"/>
                                    <xsd:enumeration value="ZrO2"/>
                                    <xsd:enumeration value="ZrSi2"/>
                                    <xsd:enumeration value="alpha-Al2O3 (corundum)"/>
                                    <xsd:enumeration value="alpha-As"/>
                                    <xsd:enumeration value="alpha-Ga"/>
                                    <xsd:enumeration value="alpha-Hg"/>
                                    <xsd:enumeration value="alpha-La"/>
                                    <xsd:enumeration value="alpha-Mn (chi-phase)"/>
                                    <xsd:enumeration value="alpha-Np"/>
                                    <xsd:enumeration value="alpha-Pa"/>
                                    <xsd:enumeration value="alpha-Po"/>
                                    <xsd:enumeration value="alpha-S"/>
                                    <xsd:enumeration value="alpha-Se"/>
                                    <xsd:enumeration value="alpha-Sm"/>
                                    <xsd:enumeration value="alpha-U"/>
                                    <xsd:enumeration value="beta-Cu3Ti"/>
                                    <xsd:enumeration value="beta-Mn"/>
                                    <xsd:enumeration value="beta-Np"/>
                                    <xsd:enumeration value="beta-Po"/>
                                    <xsd:enumeration value="beta-Se"/>
                                    <xsd:enumeration value="beta-Sn"/>
                                    <xsd:enumeration value="beta-U"/>
                                    <xsd:enumeration value="delta-CuTi"/>
                                    <xsd:enumeration value="delta-Ni3Sn4"/>
                                    <xsd:enumeration value="eta-NiSi"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="magneticState" minOccurs="0">
                            <xsd:simpleType>
                                <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value="Non-magnetic"/>
                                    <xsd:enumeration value="Paramagnetic"/>
                                    <xsd:enumeration value="Ferromagnetic"/>
                                    <xsd:enumeration value="Antiferromagnetic"/>
                                    <xsd:enumeration value="Complex magnetism"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="superconductive" type="xsd:boolean" minOccurs="0"
                            default="false"/>
                        <xsd:element name="stoichiometricCoef" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="component" maxOccurs="unbounded">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="chemical">
                                                  <xsd:complexType>
                                                  <xsd:attribute name="ref"
                                                  type="xsd:positiveInteger" use="required"/>
                                                  </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="amount" type="xsd:float"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="compositionBasis" default="number of molecules">
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value="number of molecules"/>
                                <xsd:enumeration value="mole fraction"/>
                                <xsd:enumeration value="mass fraction"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="multiphase">
                <xsd:annotation>
                    <xsd:documentation>Several phases that coexist</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="phase" minOccurs="2" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:attribute name="ref" type="xsd:positiveInteger" use="required"
                                />
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:choice>
        <xsd:attribute name="id" type="xsd:positiveInteger" use="required"/>
    </xsd:complexType>
    <xsd:simpleType name="crystalLatticeType">
        <xsd:annotation>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Cubic"/>
            <xsd:enumeration value="Tetragonal"/>
            <xsd:enumeration value="Hexagonal"/>
            <xsd:enumeration value="Rhombohedral"/>
            <xsd:enumeration value="Orthorhombic"/>
            <xsd:enumeration value="Monoclinic"/>
            <xsd:enumeration value="Triclinic"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- Dataset definition -->
    <xsd:complexType name="datasetType">
        <xsd:sequence>
            <xsd:element name="citation">
                <xsd:annotation>
                    <xsd:documentation>EAP: pointer for citation info</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:attribute name="ref" type="xsd:positiveInteger" use="required"/>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="system">
                <xsd:annotation>
                    <xsd:documentation>EAP: pointer for system/component info</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:attribute name="ref" type="xsd:positiveInteger" use="required"/>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="phase" minOccurs="1" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>EAP: pointer for phase info</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:attribute name="ref" type="xsd:positiveInteger" use="required"/>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="reference" type="tml:referenceDescriptionType2"/>
            <xsd:element name="variable" type="tml:variableType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>EAP: Availible variables</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="constantVariable" type="tml:variableType" minOccurs="0"
                maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>EAP: constant values for variables thorughout dataset</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="property" type="tml:propertyType" minOccurs="1" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>EAP: Availible properties</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="point" type="tml:pointType" maxOccurs="unbounded"/>
            <xsd:element name="expPurpose" minOccurs="0" default="Principal objective of the work">
                <xsd:simpleType>
                    <xsd:annotation>
                        <xsd:documentation>Purpose of measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Principal objective of the work"/>
                        <xsd:enumeration value="Secondary purpose (by-product of other objective)"/>
                        <xsd:enumeration
                            value="Determined for identification of a synthesized compound"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="compiler" type="xsd:string" minOccurs="0"/>
            <xsd:element name="contributor" type="xsd:string" minOccurs="0"/>
            <xsd:element name="date" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="variableType">
        <xsd:choice>
            <xsd:element name="temperature" type="tml:temperatureType"/>
            <xsd:element name="pressure" type="tml:pressureType"/>
            <xsd:element name="magneticFieldStrength" type="tml:magneticFieldStrengthType"/>
            <xsd:element name="magneticFieldDensity" type="tml:magneticFieldDensityType"/>
            <xsd:element name="massDensity" type="tml:massDensityType"/>
            <xsd:element name="molarDensity" type="tml:molarDensityType"/>
            <xsd:element name="composition" type="tml:compositionType"/>
            <!--added  variables-->
            <xsd:element name="phaseVolumeFraction" type="tml:phaseVolumeFractionType"/>
            <xsd:element name="waveLength" type="tml:waveLengthType"/>
            <xsd:element name="heatingRate" type="tml:heatingRateType"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="propertyType">
        <xsd:choice>
            <xsd:element name="criticals">
                <xsd:annotation>
                    <xsd:documentation>EAP: availible critical properties--do not require phase or component keys</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:choice>
                            <!-- Properties -->
                            <xsd:element name="criticalTemperature" type="tml:temperatureType">
                                <xsd:annotation>
                                    <xsd:documentation>EAP: measurement/calculation details-critical temperature

IUPAC: That temperature, characteristic of each gas, above which it is not possible to liquefy a given gas.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <!-- example unit: Kelvin  -->
                            <xsd:element name="criticalPressure" type="tml:pressureType">
                                <xsd:annotation>
                                    <xsd:documentation>EAP: measurement/calculation details-critical pressure

IUPAC: The minimum pressure which would suffice to liquefy a substance at its critical temperature. Above the critical pressure, increasing the temperature will not cause a fluid to vaporize to give a two-phase system.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <!-- example unit: Pascal  -->
                            <xsd:element name="criticalDensity" type="tml:massDensityType">
                                <xsd:annotation>
                                    <xsd:documentation>EAP: measurement/calculation details- critical density

Related to the Critical Point. IUPAC: The temperature and pressure at which the liquid and vapour intensive properties (density, heat capacity, etc.) become equal. It is the highest temperature (critical temperature) and pressure (critical pressure) at which both a gaseous and a liquid phase of a given compound can coexist.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <!-- example unit: kg/m^3  -->
                            <xsd:element name="criticalMolarVolume" type="tml:molarVolumeType">
                                <xsd:annotation>
                                    <xsd:documentation>EAP: measurement/calculation details-molar volume

Related to the Critical Point. IUPAC: The temperature and pressure at which the liquid and vapour intensive properties (density, heat capacity, etc.) become equal. It is the highest temperature (critical temperature) and pressure (critical pressure) at which both a gaseous and a liquid phase of a given compound can coexist.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <!-- example unit: m^3/mol  -->
                            <xsd:element name="criticalSpecificVolume"
                                type="tml:massSpecificVolumeType">
                                <xsd:annotation>
                                    <xsd:documentation>Related to the Critical Point. IUPAC: The temperature and pressure at which the liquid and vapour intensive properties (density, heat capacity, etc.) become equal. It is the highest temperature (critical temperature) and pressure (critical pressure) at which both a gaseous and a liquid phase of a given compound can coexist.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <!-- example unit: m^3/kg  -->
                            <xsd:element name="criticalCompressibilityFactor"
                                type="tml:compressibilityFactorType">
                                <xsd:annotation>
                                    <xsd:documentation>Related to the Critical Point. IUPAC: The temperature and pressure at which the liquid and vapour intensive properties (density, heat capacity, etc.) become equal. It is the highest temperature (critical temperature) and pressure (critical pressure) at which both a gaseous and a liquid phase of a given compound can coexist.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <!-- example unit: mm^3/mm^3/Kelvin  -->
                            <xsd:element name="lowerConsoluteTemperature" type="tml:temperatureType">
                                <xsd:annotation>
                                    <xsd:documentation>IUPAC: The point, with coordinates critical solution temperature and critical composition, on a temperature-composition phase diagram at which the distinction between co-existent phases vanishes. In solid-solid, solid-liquid and liquid-liquid systems both upper and lower critical solution temperatures and corresponding critical solution compositions can occur. Synonymous with consolute point.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <!-- example unit: Kelvin  -->
                            <xsd:element name="lowerConsolutePressure" type="tml:pressureType">
                                <xsd:annotation>
                                    <xsd:documentation>IUPAC: The point, with coordinates critical solution temperature and critical composition, on a temperature-composition phase diagram at which the distinction between co-existent phases vanishes. In solid-solid, solid-liquid and liquid-liquid systems both upper and lower critical solution temperatures and corresponding critical solution compositions can occur. Synonymous with consolute point.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <!-- example unit: Pascal  -->
                            <xsd:element name="upperConsoluteTemperature" type="tml:temperatureType">
                                <xsd:annotation>
                                    <xsd:documentation>IUPAC: The point, with coordinates critical solution temperature and critical composition, on a temperature-composition phase diagram at which the distinction between co-existent phases vanishes. In solid-solid, solid-liquid and liquid-liquid systems both upper and lower critical solution temperatures and corresponding critical solution compositions can occur. Synonymous with consolute point.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <!-- example unit: Kelvin  -->
                            <xsd:element name="upperConsolutePressure" type="tml:pressureType">
                                <xsd:annotation>
                                    <xsd:documentation>IUPAC: The point, with coordinates critical solution temperature and critical composition, on a temperature-composition phase diagram at which the distinction between co-existent phases vanishes. In solid-solid, solid-liquid and liquid-liquid systems both upper and lower critical solution temperatures and corresponding critical solution compositions can occur. Synonymous with consolute point.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <!-- example unit: Pascal  -->
                        </xsd:choice>
                        <xsd:choice minOccurs="1">
                            <xsd:element name="method">
                                <xsd:simpleType>
                                    <!-- methods  -->
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value="Closed Cell (static) method"/>
                                        <!--added-->
                                        <xsd:enumeration value="Derived from PVT data"/>
                                        <xsd:enumeration value="Diamond anvil cell"/>
                                        <!--added-->
                                        <xsd:enumeration value="Direct measurement"/>
                                        <xsd:enumeration value="DSC/DTA"/>
                                        <xsd:enumeration value="Extrapolated vapor pressure"/>
                                        <xsd:enumeration value="Rectilinear diameter"/>
                                        <!-- is rectilinear diameter the same as derived from PVT data or extrapolated vapor pressure -->
                                        <xsd:enumeration value="Refractive index measurements"/>
                                        <xsd:enumeration value="Relative permittivity measurements"/>
                                        <!--added-->
                                        <xsd:enumeration value="Resistive pulse heating"/>
                                        <!--added-->
                                        <xsd:enumeration value="Shadow Graph Method"/>
                                        <!--added-->
                                        <!--levitation method by which density is determined via backlighting the levitated droplet by which measurements of the volume can be taken.-->
                                        <xsd:enumeration
                                            value="Visual observation in an unstirred cell"/>
                                        <xsd:enumeration
                                            value="Visual observation in a stirred cell"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="otherMethod" type="xsd:string"/>
                            <xsd:element name="criticalEvaluation" type="tml:criticalEvaluationType"/>
                            <xsd:element name="prediction" type="tml:predictionType"/>
                        </xsd:choice>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="vaporPhaseEquilibrium">
                <xsd:annotation>
                    <xsd:documentation>EAP: availible properties associated to vapor phase equlibria-does not require phase or component keys</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:choice>
                            <!-- Properties -->
                            <xsd:element name="azeotropePressure" type="tml:pressureType"/>
                            <!-- example unit: Pa  -->
                            <xsd:element name="azeotropeTemperature" type="tml:temperatureType"/>
                            <!-- example unit: Kelvin  -->
                            <xsd:element name="boilingTemperature" type="tml:temperatureType">
                                <xsd:annotation>
                                    <xsd:documentation>IUPAC: The temperature at which a liquid boils.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <!-- example unit: Kelvin  -->
                            <xsd:element name="normalBoilingTemperature" type="tml:temperatureType">
                                <xsd:annotation>
                                    <xsd:documentation>IUPAC: The temperature at which a liquid boils. The term 'normal' in e.g. 'normal boiling temperature' means the value at a pressure of  101325 Pa.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <!-- example unit: Kelvin  -->
                            <xsd:element name="vaporPressure" type="tml:pressureType"/>
                            <!-- example unit: Pa  -->
                        </xsd:choice>
                        <xsd:choice>
                            <xsd:element name="method">
                                <xsd:annotation>
                                    <xsd:documentation>EAP: Standard method(s) of measurement</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <!-- methods  -->
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value="By X=Y"/>
                                        <xsd:enumeration value="By P(X) extreme"/>
                                        <xsd:enumeration value="By T(X) extreme"/>
                                        <xsd:enumeration value="Closed cell (Static) method"/>
                                        <xsd:enumeration value="Diaphragm manometer"/>
                                        <xsd:enumeration value="Distillation"/>
                                        <xsd:enumeration value="DSC/DTA"/>
                                        <xsd:enumeration
                                            value="Ebulliometric method (Recirculating still)"/>
                                        <xsd:enumeration value="Gas chromatography"/>
                                        <xsd:enumeration value="Inclined piston gauge"/>
                                        <xsd:enumeration value="Isochoric PVT apparatus"/>
                                        <xsd:enumeration value="Isoteniscope"/>
                                        <xsd:enumeration value="Knudsen effusion method"/>
                                        <xsd:enumeration value="Laser pulse heating"/>
                                        <xsd:enumeration value="Levitation methods"/>
                                        <xsd:enumeration value="Manometric method"/>
                                        <xsd:enumeration value="Rate of evaporation"/>
                                        <xsd:enumeration value="Transpiration method"/>
                                        <xsd:enumeration value="Torsion effusion method"/>
                                        <xsd:enumeration value="Twin ebulliometer"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="otherMethod" type="xsd:string"/>
                            <xsd:element name="criticalEvaluation" type="tml:criticalEvaluationType"/>
                            <xsd:element name="prediction" type="tml:predictionType"/>
                        </xsd:choice>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="PhaseTransition">
                <xsd:annotation>
                    <xsd:documentation>EAP: availible properties associated with phase transitions</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:choice>
                            <!-- Properties -->
                            <xsd:element name="cryoscopicConstantMole" type="tml:temperatureType"/>
                            <!-- example units: K			-->
                            <xsd:element name="cryoscopicConstantMolality" type="tml:pressureType"/>
                            <!-- example units: Pa			-->
                            <xsd:element name="cryoscopicConstantMoleFractionScale"
                                type="tml:cryoscopicConstantMoleFractionScaleType"/>
                            <!-- example units: 1/K	        -->
                            <xsd:element name="cryoscopicConstantMolalityScale"
                                type="tml:cryoscopicConstantMolalityScaleType"/>
                            <!-- example units: kg/mol/K	-->
                            <xsd:element name="liquidLiquidEquilibriumTemperature"
                                type="tml:temperatureType"/>
                            <!-- example units: K			-->
                            <xsd:element name="solidLiquidEquilibriumTemperature"
                                type="tml:temperatureType"/>
                            <!-- example units: K			-->
                            <xsd:element name="eutecticTemperature" type="tml:temperatureType"/>
                            <!-- example units: K			-->
                            <xsd:element name="monotecticTemperature" type="tml:temperatureType"/>
                            <!-- example units: K			-->
                            <xsd:element name="normalMeltingTemperature" type="tml:temperatureType"/>
                            <!-- example units: K			-->
                            <xsd:element name="peritecticTemperature" type="tml:temperatureType"/>
                            <!-- example units: K			-->
                            <xsd:element name="molarEnthalpyOfTransition" type="tml:energyType"/>
                            <!-- example units: Joule/mol	-->
                            <xsd:element name="molarEntropyOfTransition" type="tml:entropyType"/>
                            <!-- example units: Joule/mol/K	-->
                            <xsd:element name="molarEnthalpyOfVaporizationOrSublimation"
                                type="tml:energyType"/>
                            <!-- example units: Joule/mol	-->
                            <xsd:element name="molarEntropyOfVaporizationOrSublimation"
                                type="tml:entropyType"/>
                            <!-- example units: Joule/mol/K	-->
                            <xsd:element name="molarGibbsEnergyOfVaporizationOrSublimation"
                                type="tml:energyType"/>
                            <!-- example units: Joule/mol	-->
                            <xsd:element name="molarLatticeEnergyAt0K" type="tml:energyType"/>
                            <!-- example units: Joule/mol	-->
                            <xsd:element name="phaseBoundaryPressure" type="tml:pressureType"/>
                            <!-- example units: Pa			-->
                            <xsd:element name="quadrupleOrQuintuplePointTemperature"
                                type="tml:temperatureType"/>
                            <!-- example units: K			-->
                            <xsd:element name="quadrupleOrQuintuplePointPressure"
                                type="tml:pressureType"/>
                            <!-- example units: Pa			-->
                            <xsd:element name="triplePointPressure" type="tml:pressureType"/>
                            <!-- example units: Pa			-->
                            <xsd:element name="triplePointTemperature" type="tml:temperatureType"/>
                            <!-- example units: K			-->

                        </xsd:choice>
                        <xsd:choice>
                            <xsd:element name="method">
                                <xsd:annotation>
                                    <xsd:documentation>EAP: Standard method(s) of measurement</xsd:documentation>
                                </xsd:annotation>
                                <xsd:simpleType>
                                    <!-- methods  -->
                                    <xsd:restriction base="xsd:string">
                                        <xsd:enumeration value="By X=Y"/>
                                        <xsd:enumeration value="By P(X) extreme"/>
                                        <xsd:enumeration value="By T(X) extreme"/>
                                        <xsd:enumeration value="Closed cell (Static) method"/>
                                        <xsd:enumeration value="Diaphragm manometer"/>
                                        <xsd:enumeration value="Distillation"/>
                                        <xsd:enumeration value="DSC/DTA"/>
                                        <xsd:enumeration
                                            value="Ebulliometric method (Recirculating still)"/>
                                        <xsd:enumeration value="Gas chromatography"/>
                                        <xsd:enumeration value="Inclined piston gauge"/>
                                        <xsd:enumeration value="Isochoric PVT apparatus"/>
                                        <xsd:enumeration value="Isoteniscope"/>
                                        <xsd:enumeration value="Laser pulse heating"/>
                                        <xsd:enumeration value="Levitation methods"/>
                                        <xsd:enumeration value="Knudsen effusion method"/>
                                        <xsd:enumeration value="Manometric method"/>
                                        <xsd:enumeration value="Rate of evaporation"/>
                                        <xsd:enumeration value="Torsion effusion method"/>
                                        <xsd:enumeration value="Transpiration method"/>
                                        <xsd:enumeration value="Twin ebulliometer"/>

                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                            <xsd:element name="otherMethod" type="xsd:string"/>
                            <xsd:element name="CriticalEvaluation" type="tml:criticalEvaluationType"/>
                            <xsd:element name="prediction" type="tml:predictionType"/>
                        </xsd:choice>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="compositionAtPhaseEquilibrium">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:choice>
                            <xsd:element name="azeotropicCompositionMoleFraction"
                                type="tml:moleFractionType"/>
                            <!-- example unit: mol/mol  -->
                            <xsd:element name="azeotropicCompositionMassFraction"
                                type="tml:massFractionType"/>
                            <!-- example unit: kg/kg  -->
                            <xsd:element name="bunsenCoefficient" type="tml:bunsenCoefficientType">
                                <xsd:annotation>
                                    <xsd:documentation>Ratio of volume of gas at STP dissolved in a solute at STP</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="eutecticCompositionMoleFraction"
                                type="tml:moleFractionType"/>
                            <!-- example unit: mol/mol  -->
                            <xsd:element name="eutecticCompositionMassFraction"
                                type="tml:massFractionType"/>
                            <!-- example unit: kg/kg  -->
                            <xsd:element name="eutecticCompositionVolumeFraction"
                                type="tml:volumeFractionType"/>
                            <!-- example unit: m^3/m^3  -->
                            <xsd:element name="monotecticCompositionMoleFraction"
                                type="tml:moleFractionType"/>
                            <!-- example unit: mol/mol  -->
                            <xsd:element name="monotecticCompositionMassFraction"
                                type="tml:massFractionType"/>
                            <!-- example unit: kg/kg  -->
                            <xsd:element name="lowerConsoluteCompositionVolumeFraction"
                                type="tml:volumeFractionType"/>
                            <!-- example unit: m^3/m^3  -->
                            <xsd:element name="lowerConsoluteCompositionMoleFraction"
                                type="tml:moleFractionType"/>
                            <!-- example unit: mol/mol  -->
                            <xsd:element name="lowerConsoluteCompositionMassFraction"
                                type="tml:massFractionType"/>
                            <!-- example unit: kg/kg  -->
                            <xsd:element name="massConcentration"
                                type="tml:massPerVolumeSolutionType">
                                <!-- example unit: kg/m^3  -->
                                <xsd:annotation>
                                    <xsd:documentation>EAP: Mass of solute per volume of solution</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="amountRatioOfSoluteToSolvent"
                                type="tml:compositionRatioType">
                                <!-- example unit: dimensionless?  -->
                                <xsd:annotation>
                                    <xsd:documentation>EAP: equilibrium ratio of solute to solvent</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="henrysLawConstantMoleFractionScale"
                                type="tml:pressureType"/>
                            <!-- example unit: Pa  -->
                            <xsd:element name="henrysLawConstantMolalityScale"
                                type="tml:henrysLawConstantMolalityType"/>
                            <!-- example unit: Pa*kg/mol  -->
                            <xsd:element name="henrysLawConstantAmountConcentrationScale"
                                type="tml:henrysLawConstantMolarityType"/>
                            <!-- example unit: Pa* L /mol  -->
                            <!-- isn't this molarity scale-->
                            <xsd:element name="massFraction" type="tml:massFractionType"/>
                            <!-- example unit: kg/kg  -->
