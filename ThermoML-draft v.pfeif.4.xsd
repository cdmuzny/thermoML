                            <xsd:enumeration value="Interferometry"/>
                            <xsd:enumeration value="Gamma attenuation method"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="thermalPressureCoefficientType">
        <xsd:annotation>
            <xsd:documentation>EAP: used for thermal pressure coefficient, </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="Pa/K">
                <xsd:annotation>
                    <xsd:documentation>EAP: measured unit of property</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Pa/K"/>
                        <xsd:enumeration value="kPa/K"/>
                        <xsd:enumeration value="atm/K"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>EAP: Standard method(s) of measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string"> </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="thirdVirialCoefficientType">
        <xsd:annotation>
            <xsd:documentation>EAP: used in third virial coefficient, third acoustic virial coefficient, third interaction coefficient C112, third interaction coefficient C122, </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="m^6/mol^2">
                <xsd:annotation>
                    <xsd:documentation>EAP: measured unit of property</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="m^6/mol^2"/>
                        <xsd:enumeration value="dm^6/mol^2"/>
                        <xsd:enumeration value="cm^6/mol^2"/>
                        <xsd:enumeration value="nm^6/mol^2"/>
                        <xsd:enumeration value="ft^6/lb-mol^2"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>EAP: Standard method(s) of measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="derived analytically"/>
                            <xsd:enumeration value="derived graphically"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string"/>
            </xsd:choice>
            <xsd:element name="component" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>EAP: component linked to property
</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:attribute name="ref" type="xsd:positiveInteger" use="required"/>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="totalHemisphericalEmittanceType">
        <!--need to add wavelength-->
        <xsd:annotation>
            <xsd:documentation>EAP: used for total hemispherical emittance</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="dimensionless">
                <xsd:annotation>
                    <xsd:documentation>EAP: measured unit of property</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="dimensionless"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>EAP: Standard method(s) of measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="Integrating (Ulbricht) sphere"/>
                            <xsd:enumeration value="Ellipsometry"/>
                            <xsd:enumeration value="Radiance comparison"/>
                            <xsd:enumeration value="Division of amplitude photopolarimeter"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="totalNormalEmittanceType">
        <!--need to add wavelength-->
        <xsd:annotation>
            <xsd:documentation>EAP: used for total normal emittance</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="dimensionless">
                <xsd:annotation>
                    <xsd:documentation>EAP: measured unit of property</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="dimensionless"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>EAP: Standard method(s) of measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="Division of amplitude photopolarimeter"/>
                            <xsd:enumeration value="Ellipsometry"/>
                            <xsd:enumeration value="Radiance comparison"/>
                            <xsd:enumeration value="Integrating (Ulbricht) sphere"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="transitionTemperatureType">
        <xsd:annotation>
            <xsd:documentation>EAP: for all phase transition temperatures. Requires listing of invovled phases, and where applicable identified phases with phase fractions approaching zero </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="K">
                <xsd:annotation>
                    <xsd:documentation>EAP: measured unit of variable/property</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="K"/>
                        <xsd:enumeration value="C"/>
                        <xsd:enumeration value="F"/>
                        <xsd:enumeration value="R"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="scale" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>EAP: temperature scale</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="ITS-90"/>
                        <xsd:enumeration value="EPT-76"/>
                        <xsd:enumeration value="IPTS-68"/>
                        <xsd:enumeration value="IPTS-48"/>
                        <xsd:enumeration value="ITS-27"/>
                        <xsd:enumeration value="NBS-55"/>
                        <xsd:enumeration value="He3-62"/>
                        <xsd:enumeration value="He4-62"/>
                        <xsd:enumeration value="He4-58"/>
                        <xsd:enumeration value="He4-55E"/>
                        <xsd:enumeration value="He4-49"/>
                        <xsd:enumeration value="Thermodynamic"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="phaseTransitionDetails" type="tml:phaseTransitionType"/>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>EAP: Standard method(s) of measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="Platinum resistance thermometer"/>
                            <xsd:enumeration value="Optical pyrometer"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="transportNumberType">
        <!--nomethods-->
        <xsd:annotation>
            <xsd:documentation>EAP: used for transport number</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="dimensionless">
                <xsd:annotation>
                    <xsd:documentation>EAP: measured unit of property</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="dimensionless"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="otherMethod" type="xsd:string"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="viscosityType">
        <xsd:annotation>
            <xsd:documentation>EAP: used for viscosity, excess viscosity, </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="Pa s">
                <xsd:annotation>
                    <xsd:documentation>EAP: measured unit of property</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Pa s"/>
                        <xsd:enumeration value="Poise"/>
                        <xsd:enumeration value="cPoise"/>
                        <xsd:enumeration value="mPa s"/>
                        <xsd:enumeration value="microPa s"/>
                        <xsd:enumeration value="dyne s/cm^2"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>EAP: Standard method(s) of measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="capillary tube (Ostwald Ubbelohde) method"/>
                            <xsd:enumeration value="cone and plate viscometery"/>
                            <xsd:enumeration value="concentric cylinder viscometery"/>
                            <xsd:enumeration value="falling or rolling sphere viscometery"/>
                            <xsd:enumeration value="oscillating disk viscometery"/>
                            <xsd:enumeration value="vibrating wire viscometery"/>
                            <xsd:enumeration value="moving piston method"/>
                            <xsd:enumeration value="oscillating droplet dampening method"/>
                            <xsd:enumeration value="oscillating viscometer"/>
                            <xsd:enumeration value="surface laser light scattering (SLLS)"/>
                            <xsd:enumeration value="draining crucible"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="volumeFractionType">
        <xsd:annotation>
            <xsd:documentation>EAP: used for eutectic composition (vol frac), lower consolute composition (vol frac), upper consolute composition (vol frac), volume fraction,  </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="dimensionless">
                <xsd:annotation>
                    <xsd:documentation>EAP: measured unit of property</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="dimensionless"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>EAP: Standard method(s) of measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string"> </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="volumeRatioOfSoluteToSolventType">
        <xsd:annotation>
            <xsd:documentation>EAP: used in volume ratio of solute to solvent</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="dimensionless">
                <xsd:annotation>
                    <xsd:documentation>EAP: measured unit of property</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="dimensionless"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>EAP: Standard method(s) of measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string"> </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string"/>
            </xsd:choice>
            <xsd:element name="solute">
                <xsd:annotation>
                    <xsd:documentation>EAP: Solute component</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:attribute name="ref" type="xsd:positiveInteger"/>
                </xsd:complexType>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" name="solvent">
                <xsd:annotation>
                    <xsd:documentation>EAP: solvent components</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:attribute name="ref" type="xsd:positiveInteger"/>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="waveLengthType">
        <xsd:sequence>
            <xsd:element name="label" type="xsd:double" minOccurs="0"/>
            <xsd:element name="units" default="nm">
                <xsd:annotation>
                    <xsd:documentation>EAP: measured unit of property</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Angstrom"/>
                        <xsd:enumeration value="nm"/>
                        <xsd:enumeration value="micron"/>
                        <xsd:enumeration value="kX"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>EAP: Standard method(s) of measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string"> </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="otherPropertyType">
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" type="xsd:string"/>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="otherMethod" type="xsd:string"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="criticalEvaluationType">
        <xsd:sequence>
            <xsd:choice minOccurs="0">
                <xsd:element name="multiPropertyList" type="xsd:string" maxOccurs="unbounded"/>
                <xsd:element name="equationOfStateName" type="xsd:string"/>
            </xsd:choice>
            <xsd:element name="description" type="xsd:string" minOccurs="0"/>
            <xsd:element name="citation" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:attribute name="ref" type="xsd:positiveInteger" use="required"/>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="predictionType">
        <xsd:sequence>
            <xsd:element name="type">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Ab initio"/>
                        <xsd:enumeration value="Molecular dynamics"/>
                        <xsd:enumeration value="Semiempirical quantum methods"/>
                        <xsd:enumeration value="Molecular mechanics"/>
                        <xsd:enumeration value="Statistical mechanics"/>
                        <xsd:enumeration value="Corresponding states"/>
                        <xsd:enumeration value="Correlation"/>
                        <xsd:enumeration value="Group contribution"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="name" type="xsd:string" minOccurs="0"/>
            <xsd:element name="description" type="xsd:string" minOccurs="0"/>
            <xsd:element name="citation" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:attribute name="ref" type="xsd:positiveInteger" use="required"/>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <!--Uncertainty definitions-->
    <xsd:complexType name="uncertaintyType">
        <xsd:choice>
            <xsd:element name="confidenceInterval" type="tml:confidenceIntervalType"/>
            <xsd:element name="standardDeviation" type="tml:standardDeviationType"/>
            <xsd:element name="expandedUncertainty" type="tml:expandedUncertaintyType"/>
            <xsd:element name="other" type="tml:uncertaintyValueType"/>
            <xsd:element name="typeB" type="tml:uncertaintyValueType"/>
            <xsd:element name="combinedUncertainty" type="tml:uncertaintyValueType"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="uncertaintyValueType">
        <xsd:sequence>
            <xsd:element name="value" type="xsd:double"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="confidenceIntervalType">
        <xsd:complexContent>
            <xsd:extension base="tml:uncertaintyValueType">
                <xsd:attribute name="level">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:double">
                            <xsd:minInclusive value="0"/>
                            <xsd:maxInclusive value="1"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="standardDeviationType">
        <xsd:complexContent>
            <xsd:extension base="tml:uncertaintyValueType">
                <xsd:attribute name="type">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="mean"/>
                            <xsd:enumeration value="point"/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="expandedUncertaintyType">
        <xsd:complexContent>
            <xsd:extension base="tml:uncertaintyValueType">
                <xsd:attribute name="combinedUncertainty">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:double"/>
                    </xsd:simpleType>
                </xsd:attribute>
                <xsd:attribute name="effectiveDegreesOfFreedom">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:double"/>
                    </xsd:simpleType>
                </xsd:attribute>
                <xsd:attribute name="confidenceLevel">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:double"/>
                    </xsd:simpleType>
                </xsd:attribute>
                <xsd:attribute name="coverageFactor">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:double"/>
                    </xsd:simpleType>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <!--Null/number definitions-->
    <xsd:complexType name="phaseTransitionType">
        <xsd:choice minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="normalMelting" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="normalBoiling" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="sublimation" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="liquidus" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="solidus" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="solvus" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="eutecticTransformation" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="eutectoidTransformation" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="peritecticTransformation" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="peritectoidTransformation" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="monotecticTransformation" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="monotectoidTransformation" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="invariantReactionTransition" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="orderingTransition" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="lambdaSuperconductiveTransition" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="shearTransition" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="glassTransitition" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="magneticTransition" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="secondOrderPhaseTransition" type="tml:phaseBoundaryType"> </xsd:element>
            <xsd:element name="firstOrderPhaseTransition" type="tml:phaseBoundaryType">
            </xsd:element>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="phaseBoundaryType">
        <xsd:sequence>
            <xsd:element name="phaseField">
                <xsd:annotation>
                    <xsd:documentation>EAP: list of n>1 phases involved in transformation</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="phase" minOccurs="2" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>ref: phase(s)</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:attribute name="ref" type="xsd:positiveInteger" use="required"
                                />
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" name="zeroPhaseFraction">
                <xsd:annotation>
                    <xsd:documentation>EAP: list of phase(s) with fraction decreasing/increasing from zero</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="phase" minOccurs="0" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation>EAP: ref: phase(s)</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:attribute name="ref" type="xsd:positiveInteger" use="required"
                                />
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="numberOrNullType">
        <xsd:union memberTypes="xsd:double tml:nullEnumerationType"> </xsd:union>
    </xsd:simpleType>

    <xsd:simpleType name="nullEnumerationType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Outside detection limit"/>
            <xsd:enumeration value="Undefined"/>
            <xsd:enumeration value="Not measured"/>
            <xsd:enumeration value="Unknown"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="partialMolarVolumeType">
        <!--unsure about some methods-->
        <xsd:annotation>
            <xsd:documentation>type for: 
partial molar volume
 </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units" default="m^3/mol">
                <xsd:annotation>
                    <xsd:documentation>EAP: measured unit of property</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="m^3/mol"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:annotation>
                        <xsd:documentation>EAP: Standard method(s) of measurement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="Bellows volumetry"/>
                            <xsd:enumeration value="Buoyancy - hydrostatic balance"/>
                            <xsd:enumeration
                                value="Buoyancy - hydrostatic balance with magnetic float"/>
                            <xsd:enumeration
                                value="Buoyancy - hydrostatic balance with magnetic suspension - one sinker"/>
                            <xsd:enumeration
                                value="Buoyancy - hydrostatic balance with magnetic suspension - two sinkers"/>
                            <xsd:enumeration
