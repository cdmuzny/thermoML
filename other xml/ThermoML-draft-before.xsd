        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="pressureType">
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0" default="kPa"/>
            <xsd:element name="units">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="kPa"/>
                        <xsd:enumeration value="mmHg"/>
                        <xsd:enumeration value="F"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
                <xsd:element name="otherMethod" type="xsd:string"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="massDensityType">
        <xsd:sequence>
            <xsd:element name="label" type="xsd:string" minOccurs="0"/>
            <xsd:element name="units">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="kg/m^3"/>
                        <xsd:enumeration value="g/cm^3"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice minOccurs="0" maxOccurs="unbounded">
                <xsd:element name="method">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value="Pycnometric method"/>
                            <xsd:enumeration value="X-ray diffraction"/>
                            <xsd:enumeration value="Buoyancy - hydrostatic balance"/>
