                        <xsd:enumeration value="Pt/Rh"/>
                        <xsd:enumeration value="SiC"/>
                        <xsd:enumeration value="Si3N4"/>
                        <xsd:enumeration value="silica"/>
                        <xsd:enumeration value="silver"/>
                        <xsd:enumeration value="spinel"/>
                        <xsd:enumeration value="stainless_steel"/>
                        <xsd:enumeration value="tantalum"/>
                        <xsd:enumeration value="ThO2"/>
                        <xsd:enumeration value="TiO2"/>
                        <xsd:enumeration value="tungsten"/>
                        <xsd:enumeration value="W/Re"/>
                        <xsd:enumeration value="Y2O3"/>
                        <xsd:enumeration value="YSZ"/>
                        <xsd:enumeration value="zinc"/>
                        <xsd:enumeration value="ZrO2"/>
                        <xsd:enumeration value="ZrO2/Zr"/>
